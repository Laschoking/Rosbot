<launch>
 <arg name="rosbot_pro" default="true"/>
 <arg name="use_gazebo" default="false"/>
  <include unless="$(arg use_gazebo)" file="$(find astra_launch)/launch/astra.launch"/>
    <group if="$(arg use_gazebo)">
        <include file="$(find rosbot_description)/launch/rosbot_gazebo.launch"/>
            <param name="use_sim_time" value="true" />
    </group>
    <node pkg="measurement_error" type="measurement_error_node" name="measurement_error_node" output="screen">
    </node>

</launch>